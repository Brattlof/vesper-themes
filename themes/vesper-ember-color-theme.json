{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vesper Ember",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editorCursor.foreground": "#ff0000",
    "activityBar.background": "#1e1e1e",
    "activityBar.foreground": "#a0a0a0",
    "activityBar.border": "#2a2a2a",
    "sideBar.background": "#1a1a1a",
    "sideBar.foreground": "#a0a0a0",
    "sideBar.border": "#2a2a2a",
    "editorGroupHeader.tabsBackground": "#1a1a1a",
    "tab.activeBackground": "#1e1e1e",
    "tab.inactiveBackground": "#1a1a1a",
    "tab.activeForeground": "#d4d4d4",
    "tab.inactiveForeground": "#707070",
    "tab.border": "#2a2a2a",
    "statusBar.background": "#1a1a1a",
    "statusBar.foreground": "#707070",
    "statusBar.border": "#2a2a2a",
    "titleBar.activeBackground": "#1a1a1a",
    "titleBar.activeForeground": "#a0a0a0",
    "titleBar.border": "#2a2a2a",
    "editorLineNumber.foreground": "#404040",
    "editorLineNumber.activeForeground": "#707070",
    "editor.lineHighlightBackground": "#232323",
    "editor.selectionBackground": "#383838",
    "editorWidget.background": "#1a1a1a",
    "editorWidget.border": "#2a2a2a",
    "input.background": "#1a1a1a",
    "input.foreground": "#d4d4d4",
    "input.border": "#2a2a2a",
    "list.activeSelectionBackground": "#2a2a2a",
    "list.activeSelectionForeground": "#d4d4d4",
    "list.hoverBackground": "#232323",
    "focusBorder": "#3a3a3a"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a9955"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff4d4d"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff4d4d"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#858585"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#d4d4d4"
      }
    }
  ]
}
